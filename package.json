{
	"name": "ctevt-scrapper",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"start": "node dist/index.js",
		"lint": "eslint src && tsc",
		"format": "prettier --write .",
		"test": "",
		"build": "esbuild src/index.ts --bundle --platform=node --outfile=dist/index.js --format=esm --minify --packages=external",
		"dev": "run-p dev:* ",
		"dev:esbuild": "pnpm build --watch",
		"dev:node": "node --watch dist/index.js",
		"dev:tsc": "tsc --watch --preserveWatchOutput",
		"prepare": "husky install"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"src/**/*.{ts,js,jsx,tsx}": [
			"pnpm format",
			"pnpm lint"
		]
	},
	"dependencies": {
		"@commitlint/config-conventional": "^18.4.3",
		"axios": "^1.6.2",
		"commitlint": "^18.4.3",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"express-rate-limit": "^7.1.4",
		"helmet": "^7.1.0",
		"mongoose": "^8.0.1",
		"node-cron": "^3.0.3",
		"puppeteer": "^21.5.2",
		"redis": "^4.6.10",
		"uuid": "^9.0.1",
		"winston": "^3.11.0",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@types/compression": "^1.7.5",
		"@types/cors": "^2.8.16",
		"@types/express": "^4.17.21",
		"@types/node": "^20.9.2",
		"@typescript-eslint/eslint-plugin": "^6.11.0",
		"@typescript-eslint/parser": "^6.11.0",
		"esbuild": "^0.19.5",
		"eslint": "^8.54.0",
		"eslint-config-prettier": "^9.1.0",
		"husky": "^8.0.0",
		"npm-run-all": "^4.1.5",
		"typescript": "^5.2.2"
	}
}
